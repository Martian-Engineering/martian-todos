{
  "name": "@martian-todos/backend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx src/db/migrate.ts",
    "migrate:down": "tsx src/db/migrate.ts down"
  },
  "dependencies": {
    "@martian-todos/shared": "workspace:*",
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.2.1",
    "kysely": "^0.28.8",
    "pg": "^8.13.1",
    "pino": "^10.0.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "tsx": "^4.19.2"
  }
}
